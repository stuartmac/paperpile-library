@ARTICLE{Utges2021-ls,
  title       = "Ankyrin repeats in context with human population variation",
  author      = "Utgés, Javier S and Tsenkov, Maxim I and Dietrich, Noah J M and
                 MacGowan, Stuart A and Barton, Geoffrey J",
  affiliation = "Division of Computational Biology, School of Life Sciences,
